{
	"name": "evobot",
	"version": "2.6.1",
	"description": "Discord music bot built with discord.js",
	"main": "index.ts",
	"author": "Erit Islami <eritislami@gmail.com>",
	"private": true,
	"homepage": "https://github.com/eritislami/evobot",
	"repository": "github:eritislami/evobot",
	"bugs": "https://github.com/eritislami/evobot/issues",
	"engines": {
		"node": ">=16.11.0"
	},
	"scripts": {
		"dev": "nodemon index.ts | pino-pretty -i pid,hostname -t 'yyyy-mm-dd HH:MM:ss' -x emerg:90,crit:80,error:70,alert:60,warn:50,notice:40,commands:30,info:20,import:10,debug:1 -X emerg:redBright,crit:yellowBright,error:red,alert:yellow,warn:cyan,notice:white,commands:magenta,info:blue,import:green,debug:green",
		"start": "ts-node-esm index.ts | pino-pretty -i pid,hostname -t 'yyyy-mm-dd HH:MM:ss' -x emerg:90,crit:80,error:70,alert:60,warn:50,notice:40,commands:30,info:20,import:10,debug:1 -X emerg:redBright,crit:yellowBright,error:red,alert:yellow,warn:cyan,notice:white,commands:magenta,info:blue,import:green,debug:green",
		"build": "tsc --build",
		"prod": "npm run build && node ./dist/index.js",
		"commit": "cz"
	},
	"dependencies": {
		"@discordjs/voice": "^0.14.0",
		"array-move": "^3.0.1",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.3",
		"ffmpeg-static": "^4.4.1",
		"i18n": "^0.15.1",
		"lyrics-finder": "^21.0.5",
		"pino": "^8.11.0",
		"soundcloud-downloader": "^0.2.3",
		"string-progressbar": "^1.0.4",
		"youtube-sr": "~4.3.0",
		"ytdl-core": "~4.9.0",
		"ytdl-core-discord": "^1.3.1"
	},
	"devDependencies": {
		"@types/i18n": "^0.13.6",
		"@types/node": "^18.14.0",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"cz-conventional-changelog": "^3.0.1",
		"eslint": "^8.36.0",
		"eslint-config-standard-with-typescript": "^34.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-promise": "^6.1.1",
		"nodemon": "^2.0.21",
		"prettier": "^2.8.4",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"pino-pretty": "^10.0.0"
	},
	"optionalDependencies": {
		"@discordjs/opus": "^0.9.0",
		"libsodium-wrappers": "^0.7.11",
		"opusscript": "^0.0.8",
		"sodium-native": "^3.4.1",
		"tweetnacl": "^1.0.3"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog",
			"disableScopeLowerCase": false,
			"disableSubjectLowerCase": false
		}
	}
}
